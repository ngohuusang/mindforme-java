<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Payment.
    -->
    <changeSet id="20201215235826-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="family_id"
                                 baseTableName="payment"
                                 constraintName="fk_payment_family_id"
                                 referencedColumnNames="id"
                                 referencedTableName="family"/>

        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="payment"
                                 constraintName="fk_payment_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="m4m_user"/>

    </changeSet>
</databaseChangeLog>
