<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity CityData.
    -->
    <changeSet id="20201215230726-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="lang_id"
                                 baseTableName="city_data"
                                 constraintName="fk_city_data_lang_id"
                                 referencedColumnNames="id"
                                 referencedTableName="language"/>

        <addForeignKeyConstraint baseColumnNames="city_id"
                                 baseTableName="city_data"
                                 constraintName="fk_city_data_city_id"
                                 referencedColumnNames="id"
                                 referencedTableName="city"/>

    </changeSet>
</databaseChangeLog>
